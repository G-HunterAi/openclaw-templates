{
  "metadata": {
    "template": "Real Estate Lead Qualifier",
    "version": "1.0.0",
    "industry": "Real Estate",
    "author": "Hunter (OpenClaw Template)",
    "description": "Qualification questions for residential real estate buyers and sellers"
  },
  "categories": {
    "buyer": {
      "name": "Buyer Qualification",
      "description": "Questions for potential home buyers",
      "questions": [
        {
          "id": "buyer_timeline",
          "type": "multiple_choice",
          "question": "What's your timeline for purchasing a home?",
          "options": [
            {"value": "immediate", "label": "Immediately (0-30 days)", "score": 10},
            {"value": "short", "label": "1-3 months", "score": 8},
            {"value": "medium", "label": "3-6 months", "score": 6},
            {"value": "long", "label": "6-12 months", "score": 3},
            {"value": "exploratory", "label": "Just exploring/12+ months", "score": 1}
          ],
          "required": true,
          "weight": 1.5
        },
        {
          "id": "buyer_financing",
          "type": "multiple_choice",
          "question": "Are you pre-approved for a mortgage?",
          "options": [
            {"value": "pre_approved", "label": "Yes, pre-approved", "score": 10},
            {"value": "pre_qualified", "label": "Pre-qualified", "score": 7},
            {"value": "in_process", "label": "In process with lender", "score": 5},
            {"value": "not_started", "label": "Haven't started", "score": 2},
            {"value": "cash_buyer", "label": "Paying cash", "score": 10}
          ],
          "required": true,
          "weight": 2.0
        },
        {
          "id": "buyer_budget",
          "type": "range_select",
          "question": "What's your budget range?",
          "options": [
            {"value": "under_200k", "label": "Under $200K", "score": 5},
            {"value": "200k_400k", "label": "$200K - $400K", "score": 8},
            {"value": "400k_600k", "label": "$400K - $600K", "score": 9},
            {"value": "600k_1m", "label": "$600K - $1M", "score": 10},
            {"value": "1m_2m", "label": "$1M - $2M", "score": 10},
            {"value": "over_2m", "label": "Over $2M", "score": 10}
          ],
          "required": true,
          "weight": 1.0
        },
        {
          "id": "buyer_location",
          "type": "text",
          "question": "What area/neighborhoods are you interested in?",
          "placeholder": "e.g., Downtown, Suburbs, specific zip codes",
          "required": true,
          "validation": {
            "min_length": 3,
            "max_length": 200
          },
          "weight": 0.5
        },
        {
          "id": "buyer_property_type",
          "type": "multiple_select",
          "question": "What type of property are you looking for?",
          "options": [
            {"value": "single_family", "label": "Single Family Home"},
            {"value": "condo", "label": "Condo/Townhouse"},
            {"value": "multi_family", "label": "Multi-Family (2-4 units)"},
            {"value": "land", "label": "Land/Lot"},
            {"value": "commercial", "label": "Commercial Property"}
          ],
          "required": true,
          "min_select": 1,
          "max_select": 3,
          "weight": 0.5
        },
        {
          "id": "buyer_home_to_sell",
          "type": "yes_no",
          "question": "Do you need to sell your current home first?",
          "options": [
            {"value": "yes", "label": "Yes", "score": 3},
            {"value": "no", "label": "No", "score": 10}
          ],
          "required": true,
          "weight": 1.2,
          "follow_up": {
            "condition": "yes",
            "question_ids": ["buyer_home_listed"]
          }
        },
        {
          "id": "buyer_home_listed",
          "type": "yes_no",
          "question": "Is your home currently listed for sale?",
          "options": [
            {"value": "yes_listed", "label": "Yes, listed and active", "score": 10},
            {"value": "yes_pending", "label": "Yes, pending sale", "score": 10},
            {"value": "no_ready", "label": "No, but ready to list", "score": 6},
            {"value": "no_need_work", "label": "No, needs work first", "score": 3}
          ],
          "required": false,
          "depends_on": "buyer_home_to_sell",
          "weight": 1.0
        },
        {
          "id": "buyer_agent_relationship",
          "type": "multiple_choice",
          "question": "Are you currently working with a real estate agent?",
          "options": [
            {"value": "no_agent", "label": "No, looking for an agent", "score": 10},
            {"value": "browsing", "label": "No, just browsing", "score": 5},
            {"value": "yes_exclusive", "label": "Yes, exclusive agreement", "score": 0},
            {"value": "yes_casual", "label": "Yes, but not exclusive", "score": 3}
          ],
          "required": true,
          "weight": 2.0
        },
        {
          "id": "buyer_urgency",
          "type": "scale",
          "question": "How serious are you about buying? (1-10)",
          "min": 1,
          "max": 10,
          "min_label": "Just starting to look",
          "max_label": "Ready to make an offer",
          "required": true,
          "weight": 1.5
        },
        {
          "id": "buyer_contact_preference",
          "type": "multiple_choice",
          "question": "How would you prefer to be contacted?",
          "options": [
            {"value": "phone", "label": "Phone call"},
            {"value": "text", "label": "Text message"},
            {"value": "email", "label": "Email"},
            {"value": "any", "label": "Any method is fine"}
          ],
          "required": true,
          "weight": 0
        }
      ]
    },
    "seller": {
      "name": "Seller Qualification",
      "description": "Questions for potential home sellers",
      "questions": [
        {
          "id": "seller_timeline",
          "type": "multiple_choice",
          "question": "When are you planning to sell?",
          "options": [
            {"value": "asap", "label": "As soon as possible", "score": 10},
            {"value": "1_3_months", "label": "1-3 months", "score": 9},
            {"value": "3_6_months", "label": "3-6 months", "score": 7},
            {"value": "6_12_months", "label": "6-12 months", "score": 4},
            {"value": "exploring", "label": "Just exploring options", "score": 2}
          ],
          "required": true,
          "weight": 1.5
        },
        {
          "id": "seller_motivation",
          "type": "multiple_select",
          "question": "Why are you selling? (Select all that apply)",
          "options": [
            {"value": "relocating", "label": "Relocating for work"},
            {"value": "upsizing", "label": "Need more space"},
            {"value": "downsizing", "label": "Downsizing"},
            {"value": "financial", "label": "Financial reasons"},
            {"value": "lifestyle", "label": "Lifestyle change"},
            {"value": "inherited", "label": "Inherited property"},
            {"value": "testing_market", "label": "Testing the market"}
          ],
          "required": true,
          "min_select": 1,
          "scoring": {
            "relocating": 9,
            "upsizing": 9,
            "downsizing": 8,
            "financial": 10,
            "lifestyle": 7,
            "inherited": 8,
            "testing_market": 3
          },
          "weight": 1.2
        },
        {
          "id": "seller_property_address",
          "type": "text",
          "question": "What's the property address?",
          "placeholder": "Street address, City, State, ZIP",
          "required": true,
          "validation": {
            "min_length": 10,
            "max_length": 200
          },
          "weight": 0
        },
        {
          "id": "seller_property_type",
          "type": "multiple_choice",
          "question": "What type of property is it?",
          "options": [
            {"value": "single_family", "label": "Single Family Home", "score": 10},
            {"value": "condo", "label": "Condo/Townhouse", "score": 9},
            {"value": "multi_family", "label": "Multi-Family (2-4 units)", "score": 8},
            {"value": "land", "label": "Land/Lot", "score": 6},
            {"value": "commercial", "label": "Commercial Property", "score": 5}
          ],
          "required": true,
          "weight": 0.5
        },
        {
          "id": "seller_ownership",
          "type": "multiple_choice",
          "question": "Do you own the property free and clear?",
          "options": [
            {"value": "owned", "label": "Yes, owned outright", "score": 10},
            {"value": "mortgage", "label": "No, I have a mortgage", "score": 8},
            {"value": "not_sure", "label": "Not sure of equity situation", "score": 5}
          ],
          "required": true,
          "weight": 0.8
        },
        {
          "id": "seller_condition",
          "type": "multiple_choice",
          "question": "What's the property condition?",
          "options": [
            {"value": "excellent", "label": "Move-in ready/Excellent", "score": 10},
            {"value": "good", "label": "Good condition", "score": 9},
            {"value": "fair", "label": "Needs minor repairs", "score": 7},
            {"value": "poor", "label": "Needs major repairs", "score": 5},
            {"value": "fixer", "label": "Fixer-upper", "score": 6}
          ],
          "required": true,
          "weight": 0.8
        },
        {
          "id": "seller_price_expectation",
          "type": "text",
          "question": "What price are you hoping to get?",
          "placeholder": "e.g., $500,000 or not sure",
          "required": false,
          "validation": {
            "max_length": 50
          },
          "weight": 0.3
        },
        {
          "id": "seller_agent_relationship",
          "type": "multiple_choice",
          "question": "Are you currently working with a listing agent?",
          "options": [
            {"value": "no_agent", "label": "No, looking for an agent", "score": 10},
            {"value": "interviewing", "label": "Interviewing agents", "score": 8},
            {"value": "yes_exclusive", "label": "Yes, have exclusive listing", "score": 0},
            {"value": "yes_expired", "label": "Had listing, now expired", "score": 9}
          ],
          "required": true,
          "weight": 2.0
        },
        {
          "id": "seller_next_home",
          "type": "multiple_choice",
          "question": "Where will you go after selling?",
          "options": [
            {"value": "buying_local", "label": "Buying another home locally", "score": 10},
            {"value": "buying_relocate", "label": "Buying in another area", "score": 9},
            {"value": "renting", "label": "Renting", "score": 8},
            {"value": "family", "label": "Moving in with family", "score": 7},
            {"value": "not_sure", "label": "Haven't decided yet", "score": 5}
          ],
          "required": true,
          "weight": 0.8
        },
        {
          "id": "seller_urgency",
          "type": "scale",
          "question": "How motivated are you to sell? (1-10)",
          "min": 1,
          "max": 10,
          "min_label": "Exploring options",
          "max_label": "Must sell immediately",
          "required": true,
          "weight": 1.5
        },
        {
          "id": "seller_contact_preference",
          "type": "multiple_choice",
          "question": "Best way to contact you?",
          "options": [
            {"value": "phone", "label": "Phone call"},
            {"value": "text", "label": "Text message"},
            {"value": "email", "label": "Email"},
            {"value": "any", "label": "Any method is fine"}
          ],
          "required": true,
          "weight": 0
        }
      ]
    }
  },
  "lead_capture": {
    "required_fields": ["name", "email", "phone"],
    "fields": [
      {
        "id": "name",
        "type": "text",
        "label": "Full Name",
        "placeholder": "John Smith",
        "required": true,
        "validation": {
          "min_length": 2,
          "max_length": 100
        }
      },
      {
        "id": "email",
        "type": "email",
        "label": "Email Address",
        "placeholder": "john@example.com",
        "required": true,
        "validation": {
          "format": "email"
        }
      },
      {
        "id": "phone",
        "type": "tel",
        "label": "Phone Number",
        "placeholder": "(555) 123-4567",
        "required": true,
        "validation": {
          "format": "phone",
          "min_length": 10
        }
      },
      {
        "id": "lead_type",
        "type": "select",
        "label": "I'm interested in:",
        "options": [
          {"value": "buyer", "label": "Buying a home"},
          {"value": "seller", "label": "Selling a home"},
          {"value": "both", "label": "Both buying and selling"}
        ],
        "required": true
      }
    ]
  },
  "conversation_flow": {
    "greeting": "Hi! I'm here to help you {lead_type}. I'll ask a few quick questions to understand your needs better. This will only take 2-3 minutes. Ready?",
    "transition": "Great! Let's continue...",
    "completion": "Thank you! I have everything I need. {agent_name} will review your information and reach out within {response_time}.",
    "exit_early": "No problem! If you change your mind, feel free to reach back out. Have a great day!"
  }
}
